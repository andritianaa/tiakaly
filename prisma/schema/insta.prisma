model PostInsta {
    id      String   @id @default(cuid())
    date    DateTime
    url     String
    title   String
    placeId String?
    place   Place?   @relation(fields: [placeId], references: [id], onDelete: Cascade)

    TopTop1 Top[] @relation(name: "TopTop1")
    TopTop2 Top[] @relation(name: "TopTop2")
    TopTop3 Top[] @relation(name: "TopTop3")
}
